{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fable": "fable-splitter -e .jsx",
    "next": "next",
    "fableBuild": "fable-splitter App.fsproj --allFiles -e .jsx -o ./",
    "dev": "fable-splitter App.fsproj --allFiles -e .jsx -w -o ./ & next dev",
    "nextDev": "next dev",
    "build": "fable-splitter App.fsproj --allFiles -e .jsx -d && next build",
    "start": "next start"
  },
  "dependencies": {
    "lit": "^2.4.1",
    "next": "^13.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "fable-compiler": "*",
    "fable-splitter": "*"
  }
}
